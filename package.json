{
  "name": "nodebb-plugin-import-phpbb3.2",
  "version": "0.2.0",
  "description": "A phpBB3.2 to NodeBB data exporter",
  "main": "./index.js",
  "scripts": {
    "build": "babel src --out-dir=build --source-maps=inline",
    "build-watch": "babel src -v --out-dir=build --watch --source-maps=inline"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RoPP/nodebb-plugin-import-phpbb3.2"
  },
  "keywords": [
    "phpbb",
    "plugin",
    "migrate",
    "convert",
    "export",
    "import"
  ],
  "authors": [
    {
      "name": "psychobunny",
      "email": "psycho.bunny@hotmail.com"
    },
    {
      "name": "jskorepa",
      "email": "jakub@skorepa.info"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RoPP/nodebb-plugin-import-phpbb3.2/issues"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 6
          }
        }
      ]
    ],
    "env": "node"
  },
  "dependencies": {
    "async": "~0.9.0",
    "fs-extra": "~0.11.0",
    "html-entities": "",
    "lodash": "^4.17.2",
    "marked": "~0.3.2",
    "mysql": "~2.4.2",
    "node-mkdirp": "0.0.1",
    "underscore": "~1.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-env": "^1.1.4",
    "eslint": "^4.12.1"
  }
}
